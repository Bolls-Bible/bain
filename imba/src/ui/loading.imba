tag loading
	prop genesis = Date.now()
	prop speed = 1024
	prop period = 6 * speed

	def mount
		#timer = setInterval(render.bind(self), 1000 / 25)

	def unmount
		clearInterval(#timer)

	def scale delay\number
		delay *= speed
		# console.log genesis, delay, period
		return Math.cos(((Date.now() - genesis - delay) / period + 0.25) * 2 * Math.PI) + 4

	def translateX delay\number
		delay *= speed
		return 16 * Math.cos(((Date.now() - genesis - delay) / period) * 2 * Math.PI)

	def translateY delay\number
		delay *= speed
		return 8 * Math.sin(((Date.now() - genesis - delay) / period) * 2 * Math.PI)

	def rotate delay\number
		delay *= speed
		return Math.floor(((Date.now() - genesis - delay) / (period / 4)) * 360 - 270)


